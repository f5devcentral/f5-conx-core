{
    "name": "f5-conx-core",
    "author": "F5 Networks",
    "version": "0.13.0",
    "description": "F5 SDK for JavaScript with Typescript type definitions",
    "license": "SEE LICENSE IN LICENSE.txt",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "tags": [
        "f5",
        "sdk",
        "node",
        "javascript",
        "typescript"
    ],
    "keywords": [
        "f5",
        "sdk",
        "node",
        "javascript",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/f5devcentral/f5-conx-core.git"
    },
    "bugs": {
        "url": "https://github.com/f5devcentral/f5-conx-core/issues"
    },
    "scripts": {
        "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'tests/**/*.{js,ts,tsx}'",
        "watch": "tsc -watch -p ./",
        "compile": "tsc -p ./",
        "rest-server": "nodemon ./tests/testServer/restServer.ts --inspect",
        "test": "nyc mocha -r ts-node/register \"tests/*.tests.ts\"",
        "build-code-docs": "typedoc --out code_docs src",
        "build-package": "npm run compile && npm pack",
        "publish": "npm run compile && npm publish"
    },
    "dependencies": {
        "@szmarczak/http-timer": "^4.0.6",
        "ajv": "^8.9.0",
        "axios": "^0.25.0",
        "koa": "^2.13.4",
        "koa-body": "^4.2.0",
        "koa-router": "^10.1.1"
    },
    "devDependencies": {
        "@types/assert": "^1.5.4",
        "@types/mocha": "^8.2.3",
        "@types/node": "^14.14.37",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "assert": "^2.0.0",
        "eslint": "^7.23.0",
        "mocha": "^9.2.0",
        "nock": "^13.2.1",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "ts-node": "^9.1.1",
        "typedoc": "^0.22.11",
        "typescript": "^4.2.4"
    },
    "nyc": {
        "check-coverage": true,
        "lines": 80,
        "functions": 80,
        "branches": 80,
        "statements": 80,
        "reporter": [
            "html",
            "text"
        ],
        "cache": false
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended"
        ]
    },
    "mocha": {
        "timeout": 40000
    }
}
